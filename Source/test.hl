import basics.hl

var stair_char
var number
var current_stair

set dir current_stair int 1
set dir stair_char str #
inp dir number

unt dir number
cpy dir current_stair dir 0
unt dir 0
out dir stair_char str
inc dir 0 int -1
end_unt
inc dir current_stair int 1
new_line
inc dir number int -1
end_unt
